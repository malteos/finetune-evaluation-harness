# .coveragerc to control coverage.py
[run]
omit =
    */huggingface/modules/*
    */tasks/classification.py
    */tasks/ner.py
    */tasks/qa.py
    */templates/*
    */tasks/*
[coverage:paths]
source =
    finetune-evaluation-harness
    /finetune-evaluation-harness/hf_scripts
    /finetune-evaluation-harness/tests